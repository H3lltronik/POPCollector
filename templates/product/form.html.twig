{% extends 'base.html.twig' %}

{% block title %}Hello HomeController!{% endblock %}

{% block body %}
    <product-form-component submit="{{path('product_save')}}" uploadEndpoint="{{path('product_image-upload')}}"
    genresprop="{{genres|serialize}}" distribuitorlabel="{{distribuitorLabel|default("Distribuidor")}}" 
    authorlabel="{{authorLabel|default("Autor")}}" namelabel="{{nameLabel|default("Nombre")}}"
    producttypeprop="{{productType|serialize}}" formatsprop="{{formats|serialize}}" editionsprop="{{editions|serialize}}"
    productID="{{productID}}" entityprop="{{entity|serialize}}"></product-form-component>
{% endblock %}

